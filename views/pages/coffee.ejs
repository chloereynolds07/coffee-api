<%- include('../partials/header') %>

<main class="main" id="coffeeMain">
    <div class="container">
        <div class="row">
            <% data.forEach(coffee => { %>
                <div class="col-md-4">
                    <h4 class="py-2 coffee-title">
                        <a href="/<%= path %>/<%= coffee.id %>" class="coffee-title"><%= coffee.title %></a>
                    </h4>
                    <figure class="figure coffee-img">
                        <img src="<%= coffee.image %>" alt="<%= coffee.title %>" class="figure-img img-fluid w-50 rounded">
                    </figure>
                    <!-- <figcaption class="figure-caption coffee-desc"><%= coffee.description %></figcaption> -->
                </div>
            <%  }) %>
        </div>
    </div>
</main>



<%- include('../partials/footer') %>